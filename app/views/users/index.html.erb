<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
  <thead>
  <tr>
    <th><%= User.human_attribute_name(:email) %></th>
  </tr>

  <tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
  </tr>
  <% end %>
  </tbody>
  </thead>
</table>

<%= paginate @users %>
